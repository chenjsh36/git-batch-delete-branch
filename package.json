{
  "name": "@shopee/git-batch-delete-branch",
  "version": "1.0.0",
  "description": "A CLI tool for batch deleting Git local branches with interactive selection and filtering options",
  "main": "dist/index.js",
  "bin": {
    "git-batch-delete-branch": "dist/cli.js",
    "git-branch-clean": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "setup": "chmod +x scripts/setup.sh && ./scripts/setup.sh",
    "example": "node examples/basic-usage.js",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "git",
    "branch",
    "delete",
    "batch",
    "cli",
    "typescript",
    "node"
  ],
  "author": "Jiansheng Chen <chenjsh36@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chenjsh36/git-batch-delete-branch.git"
  },
  "bugs": {
    "url": "https://github.com/chenjsh36/git-batch-delete-branch/issues"
  },
  "homepage": "https://github.com/chenjsh36/git-batch-delete-branch#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
